{
  "name": "pixie",
  "version": "0.1.0",
  "description": "Librer√≠a de Web Components con Lit",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "dev": "tsup src/index.ts --dts --format=esm,cjs --watch --clean",
    "build": "tsup src/index.ts --dts --format=esm,cjs --minify --sourcemap --clean",
    "check": "tsc --noEmit"
  },
  "peerDependencies": {
    "lit": "^3.0.0"
  },
  "devDependencies": {
    "lit": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "keywords": ["web-components", "lit", "ui", "library"],
  "license": "MIT",
  "repository": "github:pvckstein/pixie"
}
