{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "useDefineForClassFields": true
  },
  "include": ["src"]
}
